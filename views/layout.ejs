    <script src="/js/dependencies/materialize/slider.js"></script>
    <script src="/js/dependencies/materialize/tabs.js"></script>
    <script src="/js/dependencies/materialize/toasts.js"></script>
    <script src="/js/dependencies/materialize/tooltip.js"></script>
